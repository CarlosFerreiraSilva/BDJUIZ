@{
}

<div id="divtabela">
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">DocumentaçãoID</th>
                <th scope="col">Nome</th>
                <th scope="col">TipoJuiz</th>
                <th scope="col">Salario</th>
                <th scope="col">CursoSuperior</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in @ViewBag.listaDocumentoJuiz)
            {
                <tr>
                    <td>@item.DocumentaçãoID</td>
                    <td>@item.Nome</td>
                    <td>@item.TipoJuiz</td>
                    <td>@item.Salario</td>
                    <td>@item.CursoSuperior</td>
                </tr>
            }
        </tbody>
    </table>
</div>
